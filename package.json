{
  "name": "xz",
  "version": "1.1.1",
  "description": "node bindings for the xz compression library",
  "keywords": [
    "compression",
    "xz"
  ],
  "author": "Robey Pointer <robeypointer@gmail.com>",
  "homepage": "https://github.com/robey/node-xz",
  "repository": {
    "type": "git",
    "url": "git://github.com/robey/node-xz.git"
  },
  "licenses": [
    {
      "type": "Apache License 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.txt"
    }
  ],
  "scripts": {
    "clean": "rm -rf lib build",
    "build": "node-gyp configure build",
    "prepublish": "6to5 --source-maps --out-dir lib src && 6to5 --source-maps --out-dir test/lib test/src",
    "test": "mocha -R spec --colors test/lib --timeout 30000"
  },
  "dependencies": {
    "nan": "*"
  },
  "devDependencies": {
    "6to5": "*",
    "mocha": "*",
    "mocha-sprinkles": "*",
    "should": "*"
  },
  "main": "./lib/xz.js",
  "files": [
    "binding.gyp",
    "Changelog.md",
    "deps",
    "lib",
    "LICENCE.txt",
    "package.json",
    "README.md",
    "src/c++"
  ]
}
